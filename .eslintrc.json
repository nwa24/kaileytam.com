{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "import/prefer-default-export": "off",
    "import/no-unresolved": { "commonjs": true },
    "react/jsx-one-expression-per-line": "allow",
    "react/forbid-prop-types": ["any", "array"]
  }
}
